{
  "name": "@abstract-names/sdk",
  "version": "0.1.0",
  "description": "React SDK for Abstract Names - name resolution and profiles",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "keywords": [
    "abstract",
    "names",
    "ens",
    "wagmi",
    "react",
    "ethereum",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/react": "^19.0.0",
    "react": "^19.1.0",
    "tsup": "^8.3.5",
    "typescript": "^5.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/abstract-names"
  }
}
